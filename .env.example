# Database
DATABASE_URL=postgresql://username:password@localhost:5432/depin

# App Settings
DEBUG=True
ENVIRONMENT=development

# CORS Settings (adjust for production)
ALLOWED_ORIGINS=*

# Logging
LOG_LEVEL=INFO

# Discord Bot Integration
DISCORD_API_AUTH_TOKEN=your_discord_bot_api_token_here
DISCORD_BOT_WEBHOOK_URL=http://localhost:8080/webhook/send-message
DISCORD_NOTIFICATION_CHANNEL=https://discord.com/api/webhooks/webhook_id/webhook_token

# Discord OAuth2 Configuration
OAUTH_CLIENT_ID=your_oauth_client_id_here
OAUTH_CLIENT_SECRET=your_oauth_client_secret_here
OAUTH_REDIRECT_URI=http://localhost:8000/api/v1/lite/public/discord/callback
STATE_SIGNING_KEY=your_32_plus_character_hmac_signing_key_here
ALLOWED_GUILD_IDS=123456789012345678,987654321098765432
VERIFY_SUCCESS_TTL_DAYS=90
VERIFY_FAIL_TTL_MINUTES=10
MIN_ACCOUNT_AGE_DAYS=7
BOT_TOKEN=your_bot_token_here_optional
OAUTH_BASE=https://discord.com/api

# Redis (optional)
REDIS_URL=redis://localhost:6379